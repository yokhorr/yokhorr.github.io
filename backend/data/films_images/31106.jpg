    Handlebars.registerHelper('wisePlacesNumber', function(number) {
        var titles = ["билет", "билета", "билетов"];
        var cases = [2, 0, 1, 1, 1, 2];

        return titles[(number % 100 > 4 && number % 100 < 20) ? 2 : cases[Math.min(number % 10, 5)]];
    });
</script>
